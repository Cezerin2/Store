{
  "name": "cezerin2-store",
  "version": "0.35.0",
  "description": "Cezerin2 is React and Node.js based eCommerce platform. https://cezerin.org",
  "keywords": [
    "cezerin2",
    "cezerin",
    "ecommerce",
    "shopping cart",
    "shop",
    "cms"
  ],
  "homepage": "https://cezerin.org",
  "bugs": "https://github.com/Cezerin2/cezerin2-store/issues",
  "license": "Unlicense",
  "author": "Restmount, Himadu, Community",
  "main": "dist/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cezerin2/cezerin2-store"
  },
  "scripts": {
    "test": "jest",
    "tscompile": "tsc",
    "compile": "babel build -d dist --presets react --plugins transform-class-properties --quiet",
    "compile:watch": "concurrently -n storeWatcher,themeWatcher \"babel src -d dist --presets react --plugins transform-class-properties --watch\" \"babel theme/src -d theme/dist --presets react --plugins transform-class-properties --watch\"",
    "webpack:store": "webpack --config webpack.config.store.js --mode production",
    "webpack:store:watch": "webpack -d --config webpack.config.store.js --mode development --watch",
    "theme:install": "./scripts/theme-install.sh",
    "theme:export": "./scripts/theme-export.sh",
    "theme:build": "babel theme/src -d theme/dist --presets react --plugins transform-class-properties --quiet && npm run webpack:store",
    "build": "npm run compile && npm run theme:build",
    "build:watch": "concurrently -n compile,webpackStore \"npm run compile:watch\" \"npm run webpack:store:watch\"",
    "dev:watch": "concurrently -n compile,webpackStore,appRun \"npm run compile:watch\" \"npm run webpack:store:watch\" \"pm2 start process.json\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"src/**/*.js\" \"theme/**/*.js\"",
    "precommit": "lint-staged",
    "start": "node -r esm dist/server/index.js",
    "pmstart": "pm2 start process.json"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write .",
      "git add ."
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cezerin2-client": "^0.36.0",
    "cookie-parser": "^1.4.5",
    "cssnano": "^4.1.10",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "fs-extra": "^9.0.0",
    "handlebars": "^4.7.6",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "lscache": "^1.3.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.0",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^10.2.2",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scroll": "^1.7.16",
    "react-sortable-hoc": "^1.11.0",
    "react-stripe-elements": "^6.1.1",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.2",
    "redux-form-material-ui": "^4.3.4",
    "redux-thunk": "^2.3.0",
    "response-time": "^2.3.2",
    "sitemap": "^6.1.0",
    "slug": "^2.1.1",
    "stripe": "^8.39.1",
    "theme": "./theme",
    "typescript": "^3.9.2",
    "ua-parser-js": "^0.7.21",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.5.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^4.0.4",
    "lint-staged": "^10.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "pm2": "^4.4.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.4",
    "sass-loader": "^8.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "workbox-webpack-plugin": "^5.1.2"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  }
}
